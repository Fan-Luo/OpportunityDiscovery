MATCH (c1:Concept)-[r1]->(c2:Concept)-[r2]->(c3:Concept)
WHERE NOT (c1)-[]->(c3) AND toInteger(r1.EDGE_YEAR) < 2012 AND toInteger(r2.EDGE_YEAR) < 2012 AND toInteger(r1.DOCS_SEEN)>=2 AND toInteger(r2.DOCS_SEEN)>=2 AND r1.NEGATED_COUNT = 0 AND r2.NEGATED_COUNT = 0 AND c1.AVE_IDF >= 1.0 AND c2.AVE_IDF >= 1.0 AND c3.AVE_IDF >= 1.0 
RETURN c1.NODE_HASH,c2.NODE_HASH,c3.NODE_HASH,r1.EDGE_DEDUPLICATION_HASH,r2.EDGE_DEDUPLICATION_HASH;